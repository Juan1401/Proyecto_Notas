@model Proyecto_Notas.Models.Nota

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Nota</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Nota
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nota1)
        </dd>
        <dt class = "col-sm-2">
            Estudiante
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdEstudianteNavigation.Nombre)
            @Html.DisplayFor(model => model.IdEstudianteNavigation.Apellido)
        </dd>
        <dt class = "col-sm-2">
            Materia
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdMateriaNavigation.NombreMateria)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdNota">Editar</a> |
    <a asp-action="Index">Volver</a>
</div>
